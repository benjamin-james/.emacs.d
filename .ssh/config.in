ServerAliveInterval 120

Match exec "echo %h | grep -qE '^login[01234]$'"
    HostName %h
    User @broad@
    ControlMaster yes

Host jump.csail.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    User @csail@

Host *.csail.mit.edu !jump.csail.mit.edu 128.52.* 128.30.* 128.31.*
    ProxyJump jump.csail.mit.edu
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    GSSAPIKeyExchange yes
    User @csail@

Host athena.dialup.mit.edu
    HostName athena.dialup.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    User @mit@

Host luria.mit.edu
    HostName 10.159.3.125
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    ProxyJump eofe7.mit.edu
    User @mit@
    PubkeyAcceptedKeyTypes +ssh-rsa

Host openmind.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    User @mit@
    PubkeyAcceptedKeyTypes +ssh-rsa

Host eofe7.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    User @mit@
    PubkeyAcceptedKeyTypes +ssh-rsa

Host eofe8.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    User @mit@
    PubkeyAcceptedKeyTypes +ssh-rsa

Match exec "echo %h | grep -qE '^b[0-9]+$'"
    HostName %h
    ProxyJump luria.mit.edu
    GSSAPIAuthentication yes
    GSSAPIKeyExchange yes
    VerifyHostKeyDNS yes
    ControlMaster yes
    User @mit@

Match exec "echo %h | grep -qE '^kellislab[0-9]+'$"
    HostName %h.csail.mit.edu
    ProxyJump jump.csail.mit.edu
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    GSSAPIKeyExchange yes
    User @csail@